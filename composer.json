{
    "name": "azgooon/currencylayer",
    "description": "PHP client for currencylayer.com",
    "keywords": [
        "orkhanahmadov",
        "currency",
        "currencylayer"
    ],
    "homepage": "https://github.com/azgooon/currencylayer",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Orkhan Ahmadov",
            "email": "ahmadov90@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {
        "blastcloud/guzzler": "^2.0.3",
        "phpunit/phpunit": "9.3.3"
    },
    "autoload": {
        "psr-4": {
            "azgooon\\Currencylayer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "azgooon\\Currencylayer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
